{
  "name": "<%= packageName %>",
  "pfelement": {
    "className": "<%= elementClassName %>",
    "elementName": "<%= elementName %>"
  },
  "version": "<%= pfelementVersion %>",
  "description": "<%= readmeName %> element for PatternFly Elements",
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  <% if (isPfelement) { %>"scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "../../node_modules/.bin/gulp dev",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json elements/<%= elementName %>/test/"
  },<% } else { %>"scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "npm run dev",
    "dev": "gulp dev",
    "build": "gulp",
    "test": "wct"
  },<% } %>
  "author": {
    "name": "<%= author %>"
  },
  "license": "MIT",
  "dependencies": {
    <% if (sassLibraryPkg) { %>"<%= sassLibraryPkg %>": "<%= pfeSassVersion %>",<% } %>
    "@patternfly/pfelement": "<%= pfelementVersion %>"
  },<% if (!isPfelement) { %>
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "decomment": "^0.9.2",
    "gulp": "^4.0.0",
    "gulp-banner": "^0.1.3",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.6.5",
    "node-sass": "^4.11.0",
    "requirejs": "^2.3.6",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "strip-css-comments": "^3.0.0",
    "trim": "0.0.1",
    "wct-browser-legacy": "^1.0.1",
    "web-component-tester": "^6.7.1"
  },<% } %>
  "generator-pfelement-version": "<%= generatorPfelementVersion %>"
}
