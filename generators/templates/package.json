{
  "name": "@rhelements/<%= elementName %>",
  "rhelement": {
    "className": "<%= elementClassName %>",
    "elementName": "<%= elementName %>"
  },
  "version": "<%= rhelementVersion %>",
  "description": "<%= readmeName %> element for RHElements",
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "../../node_modules/.bin/gulp dev",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@rhelements/<%= elementName %>/test/"
  },
  "author": {
    "name": "<%= author %>"
  },
  "license": "MIT",
  "dependencies": {
    <% if (sassLibraryPkg) { %>"<%= sassLibraryPkg %>": "<%= rhSassVersion %>",<% } %>
    "@rhelements/rhelement": "<%= rhelementVersion %>"
  },
  "generator-rhelement-version": "<%= generatorRhelementVersion %>"
}
